        <script>
            function triggerFileInput() {
                document.getElementById('UploadedFiles').click();
            }

            function previewPhotos(input) {
                const photoPreview = document.getElementById('photoPreview');
                const warningText = document.getElementById('durationWarning');
                const currentPhotos = photoPreview.getElementsByClassName('image-preview-container');
                const files = Array.from(input.files);

                const totalPhotos = currentPhotos.length + files.length;

                if (totalPhotos > 60) {
                    alert("Maksimum 60 fotoğraf yükleyebilirsiniz!");
                    return;
                }

                warningText.style.display = "none"; // Uyarı metnini gizle

                files.forEach((file) => {
                    const loadingTime = Math.floor(Math.random() * 6000) + 1000;
                    const div = document.createElement('div');
                    div.classList.add('image-preview-container');

                    div.innerHTML = `
                                        <div class="loading-animation">
                                            <div class="spinner"></div>
                                            <p>Yükleniyor...</p>
                                        </div>
                                    `;
                    photoPreview.appendChild(div);

                    setTimeout(() => {
                        const reader = new FileReader();
                        reader.onload = function (e) {
                            div.innerHTML = `
                                                <span class="vitrin-label" style="display: ${currentPhotos.length === 0 ? 'block' : 'none'};">Vitrin</span>
                                                <img src="${e.target.result}" alt="Photo" />
                                                <span class="delete-icon" onclick="removePhoto(this)">✖</span>
                                            `;
                        };
                        reader.readAsDataURL(file);
                    }, loadingTime);
                });

                if (photoPreview.children.length > 0) {
                    document.getElementById('deleteAllButton').classList.remove('d-none');
                }

                enableDragAndDrop();
            }

            function removePhoto(element) {
                const photoPreview = document.getElementById('photoPreview');
                const parentDiv = element.parentElement;
                photoPreview.removeChild(parentDiv);

                updateVitrinLabel();

                if (photoPreview.children.length === 0) {
                    document.getElementById('deleteAllButton').classList.add('d-none');
                    document.getElementById('durationWarning').style.display = "block"; // Uyarı metnini tekrar göster
                }
            }

            function removeAllPhotos() {
                const photoPreview = document.getElementById('photoPreview');
                photoPreview.innerHTML = '';
                document.getElementById('deleteAllButton').classList.add('d-none');
                document.getElementById('durationWarning').style.display = "block"; // Uyarı metnini tekrar göster
            }

            function updateVitrinLabel() {
                const photoPreview = document.getElementById('photoPreview');
                const previewContainers = photoPreview.getElementsByClassName('image-preview-container');

                Array.from(previewContainers).forEach(container => {
                    const vitrinLabel = container.querySelector('.vitrin-label');
                    if (vitrinLabel) vitrinLabel.style.display = 'none';
                });

                if (previewContainers.length > 0) {
                    const firstVitrinLabel = previewContainers[0].querySelector('.vitrin-label');
                    if (firstVitrinLabel) firstVitrinLabel.style.display = 'block';
                }
            }

            function enableDragAndDrop() {
                const photoPreview = document.getElementById('photoPreview');
                let draggedElement = null;

                photoPreview.addEventListener('dragstart', (e) => {
                    draggedElement = e.target.closest('.image-preview-container');
                    e.dataTransfer.effectAllowed = 'move';
                });

                photoPreview.addEventListener('dragover', (e) => {
                    e.preventDefault();
                    e.dataTransfer.dropEffect = 'move';
                });

                photoPreview.addEventListener('drop', (e) => {
                    e.preventDefault();
                    const target = e.target.closest('.image-preview-container');
                    if (target && draggedElement !== target) {
                        const allItems = Array.from(photoPreview.children);
                        const draggedIndex = allItems.indexOf(draggedElement);
                        const targetIndex = allItems.indexOf(target);

                        if (draggedIndex < targetIndex) {
                            photoPreview.insertBefore(draggedElement, target.nextSibling);
                        } else {
                            photoPreview.insertBefore(draggedElement, target);
                        }
                    }
                });
            }
        </script>




