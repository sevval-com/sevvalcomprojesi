    <script>

                    // Script.js
            document.getElementById('TasınmazNumarasi').addEventListener('blur', async function() {
                const tasinmazNo = this.value;
                const userType = document.getElementById('GayrimenkulSahibi').value;
                const statusSpan = document.getElementById('tasinmazDogrulamaStatus');

                if(userType === 'TC Vatandaşı' && tasinmazNo) {
                    try {
                        const response = await fetch(`/Ilan/CheckTasinmazAuthorization?tasinmazNo=${tasinmazNo}`);
                        const data = await response.json();

                        if(!data.isAuthorized) {
                            showError('TasınmazNumarasi', 'Bu taşınmaz için yetkiniz yok');
                            document.getElementById('submitButton').disabled = true;
                        } else {
                            document.getElementById('submitButton').disabled = false;
                            const errorSpan = document.querySelector(`[data-valmsg-for="TasınmazNumarasi"]`);
                            errorSpan.textContent = '';
                            // Başarılı durumu göster
                            statusSpan.textContent = 'Doğrulama Başarılı';
                            statusSpan.className = '';
                            statusSpan.style.backgroundColor = '#e6ffe6';
                            statusSpan.style.color = '#28a745';
                            statusSpan.style.padding = '3px 8px';
                            statusSpan.style.borderRadius = '3px';
                            statusSpan.style.display = 'inline-block';
                            statusSpan.style.marginTop = '5px';
                        }
                    } catch(error) {
                        console.error('Doğrulama hatası:', error);
                    }
                }
            });

            function showError(fieldId, message) {
                const errorSpan = document.querySelector(`[data-valmsg-for="${fieldId}"]`);
                errorSpan.textContent = message;
                errorSpan.style.display = 'block';
            }

    </script>

