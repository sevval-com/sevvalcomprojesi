<script>
    document.getElementById("submitButton").addEventListener("click", function () {
        // Formu seç
        const form = document.getElementById("myForm");

        // Required alanları kontrol et
        const requiredFields = form.querySelectorAll("[required]");
        let allFilled = true;

        requiredFields.forEach(function (field) {
            if (!field.value.trim()) {
                allFilled = false;
                field.style.borderColor = "red"; // Boş alanları kırmızı yap
                field.scrollIntoView({ behavior: "smooth", block: "center" }); // Boş alanları ekrana getir
            } else {
                field.style.borderColor = ""; // Doldurulmuş alanları normal hale getir
            }
        });

        // Eğer tüm required alanlar doluysa, mesajı göster ve formu gönder
        if (allFilled) {
            const loadingMessage = document.getElementById("loadingMessage");
            const backdrop = document.getElementById("backdrop");
            backdrop.style.display = "block"; // Yarı saydam arka planı göster
            loadingMessage.style.display = "block"; // Yükleniyor mesajını göster

            // 2 saniye sonra formu gönder
            setTimeout(function () {
                form.submit(); // Formu gönder
            }, 2000);
        }
    });
</script>


