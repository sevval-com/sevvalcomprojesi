    <script>
        const forbiddenWords = ["salak", "aptal", "mal", "bok", "göt"];

        function checkForForbiddenWords(textarea) {
            const text = textarea.value;

            // Split text into words
            const words = text.split(/\s+/);

            // Yeni metin için dizi oluştur
            const newWords = [];

            let containsForbidden = false; // Yasaklı kelime kontrolü için flag

            // Her kelimeyi kontrol et
            for (let i = 0; i < words.length; i++) {
                const word = words[i].toLowerCase(); // Küçük harfe çevir

                if (forbiddenWords.includes(word)) {
                    // Eğer yasaklı kelime bulunursa
                    containsForbidden = true; // Flag'i true yap
                    continue; // Kelimeyi atla
                }
                newWords.push(words[i]); // Yasaklı kelime değilse yeni metne ekle
            }

            // Eğer yasaklı kelime bulunursa
            if (containsForbidden) {
                textarea.value = newWords.join(' '); // Güncellenmiş metni textarea'ya yerleştir
                // Kelimenin renk değiştirmesi için gerekli işlemleri burada yap
                textarea.classList.add('forbidden'); // Yasaklı kelime sınıfını ekle
            } else {
                textarea.classList.remove('forbidden'); // Eğer yasaklı kelime yoksa sınıfı kaldır
            }
        }
    </script>


