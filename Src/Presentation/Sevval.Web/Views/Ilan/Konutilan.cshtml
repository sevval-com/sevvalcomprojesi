<script>
function isNumberKey(evt) {
    var charCode = evt.which ? evt.which : evt.keyCode;

    // Sadece sayılara ve noktaya izin ver
    if (charCode != 46 && (charCode < 48 || charCode > 57)) {
        return false;
    }
    return true;
}

function validateBrutMetrekare() {
    var brutMetrekareInput = document.getElementById("BrutMetrekare");
    var brutMetrekare = brutMetrekareInput.value;

    // Brüt metrekare kontrolü
    if (brutMetrekare && brutMetrekare < 5) {
        brutMetrekareInput.value = "";
        document.getElementById("brutMetrekareHata").style.display = "block";
    } else {
        document.getElementById("brutMetrekareHata").style.display = "none";
    }
    
    validateNetVsBrut();
}

function validateNetMetrekare() {
    var netMetrekareInput = document.getElementById("NetMetrekare");
    var netMetrekare = netMetrekareInput.value;

    // Net metrekare kontrolü
    if (netMetrekare && netMetrekare < 5) {
        netMetrekareInput.value = "";
        document.getElementById("netMetrekareHata").style.display = "block";
    } else {
        document.getElementById("netMetrekareHata").style.display = "none";
    }

    validateNetVsBrut();
}

function validateNetVsBrut() {
    var brutMetrekare = parseFloat(document.getElementById("BrutMetrekare").value) || 0;
    var netMetrekare = parseFloat(document.getElementById("NetMetrekare").value) || 0;

    // Net metrekare, brüt metrekareden büyükse hata göster
    if (netMetrekare > brutMetrekare) {
        document.getElementById("metrekareHata").style.display = "block";
    } else {
        document.getElementById("metrekareHata").style.display = "none";
    }
}
</script>




