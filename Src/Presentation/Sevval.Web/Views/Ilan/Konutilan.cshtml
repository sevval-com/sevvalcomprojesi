            <input type="text" id="displayPrice" name="displayPrice" class="form-control"
                   required onblur="validatePrice(this)" oninput="sanitizeInput(this)" />
            <input type="hidden" id="Price" name="Price" />
            <div id="fiyatHataMin" class="text-danger" style="display: none;">
                En az 1.000₺ olmalıdır.
            </div>
            <div id="fiyatHataMax" class="text-danger" style="display: none;">
                En fazla 500.000.000₺ olmalıdır.
            </div>
        </div>

        <script>
            function sanitizeInput(input) {
                // Nokta ve virgülleri temizle
                input.value = input.value.replace(/[.,]/g, '');
            }

            function formatNumberWithDots(number) {
                return number.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ".");
            }

            function validatePrice(input) {
                const minPrice = 1000;
                const maxPrice = 500000000;
                const fiyatHataMin = document.getElementById('fiyatHataMin');
                const fiyatHataMax = document.getElementById('fiyatHataMax');
                const hiddenPriceInput = document.getElementById('Price');

                // Son girişten temizlenmiş değer
                const rawValue = input.value.replace(/[.,]/g, '');
                const price = parseInt(rawValue, 10) || 0;

                // Hata mesajlarını ve renkleri sıfırla
                fiyatHataMin.style.display = 'none';
                fiyatHataMax.style.display = 'none';
                input.style.color = '';

                // Geçersiz karakter kontrolü
                if(isNaN(price)) {
                    input.value = '';
                    hiddenPriceInput.value = '';
                    return;
                }

                // Fiyat kontrolü
                if(price < minPrice) {
                    fiyatHataMin.style.display = 'block';
                    input.style.color = 'red';
                    input.value = '';
                    hiddenPriceInput.value = '';
                } else if(price > maxPrice) {
                    fiyatHataMax.style.display = 'block';
                    input.style.color = 'red';
                    input.value = '';
                    hiddenPriceInput.value = '';
                } else {
                    // Formatlı değeri göster ama saklanan değeri raw tut
                    input.value = formatNumberWithDots(price);
                    hiddenPriceInput.value = price;
                }
            }
        </script>



