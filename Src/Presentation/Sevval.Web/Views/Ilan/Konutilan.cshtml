<!-- jQuery kütüphanesi -->
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>

<script>
    // Belge yüklendiğinde çalışacak fonksiyon
    $(document).ready(function () {
        // JSON dosyasından şehir, ilçe, mahalle/köy verilerini yükle
        $.getJSON("/data/TR-İl-İlçe-Mahalle_(City-County-Neighborhood).json", function (data) {
            // Şehirleri sadece baş harfe göre alfabetik sıralayın
            data.sort((a, b) => a.Il[0].localeCompare(b.Il[0]));

            // Şehirleri doldur
            data.forEach(function (city) {
                $('#sehir').append(new Option(city.Il, city.Il));
            });

            // Şehir seçimi değiştiğinde ilçe ve mahalle seçeneklerini güncelle
            $('#sehir').change(function () {
                var selectedCity = $(this).val();
                var selectedCityData = data.find(c => c.Il === selectedCity);

                // İlçe menüsünü temizle
                $('#semt').empty().append('<option value="">İlçe Seçiniz</option>');
                // Mahalle menüsünü temizle
                $('#mahalleKoy').empty().append('<option value="">Mahalle/Köy Seçiniz</option>');

                if (selectedCityData) {
                    // İlçeleri sadece baş harfe göre alfabetik sıralayın
                    selectedCityData.Ilce.sort((a, b) => a.Ilce[0].localeCompare(b.Ilce[0]));

                    selectedCityData.Ilce.forEach(function (district) {
                        $('#semt').append(new Option(district.Ilce, district.Ilce));
                    });
                }
            });

            // Semt değiştiğinde mahalle seçeneklerini güncelle
            $('#semt').change(function () {
                var selectedCity = $('#sehir').val();
                var selectedDistrict = $(this).val();
                var selectedCityData = data.find(c => c.Il === selectedCity);
                var selectedDistrictData = selectedCityData.Ilce.find(d => d.Ilce === selectedDistrict);

                // Mahalle menüsünü temizle
                $('#mahalleKoy').empty().append('<option value="">Mahalle/Köy Seçiniz</option>');

                if (selectedDistrictData) {
                    selectedDistrictData.Semt.forEach(function (neighborhood) {
                        // Mahalleleri sadece baş harfe göre alfabetik sıralayın
                        neighborhood.Mahalle.sort((a, b) => a.Mahalle[0].localeCompare(b.Mahalle[0]));

                        neighborhood.Mahalle.forEach(function (mah) {
                            // Mahalle ismini seçenekler olarak ekleyin
                            $('#mahalleKoy').append(new Option(mah.Mahalle, mah.Mahalle));
                        });
                    });
                }
            });

            // Şehir, semt veya mahalle/köy seçimi değiştiğinde konum inputunu güncelle
            $('#sehir, #semt, #mahalleKoy').on('change', function () {
                var sehir = $('#sehir').val();
                var semt = $('#semt').val();
                var mahalle = $('#mahalleKoy').val();

                // Konum bilgilerini birleştir
                var konum = [];
                if (sehir) {
                    konum.push(sehir);
                }
                if (semt) {
                    konum.push(semt);
                }
                if (mahalle) {
                    konum.push(mahalle);
                }

                // Eğer konum bilgisi varsa, virgülle ayırarak input alanına yaz
                if (konum.length > 0) {
                    $('#location-input').val(konum.join(', ')); // Virgülle ayırarak
                } else {
                    $('#location-input').val(''); // Konum yoksa inputu temizle
                }
            });
        });
    });
</script>



