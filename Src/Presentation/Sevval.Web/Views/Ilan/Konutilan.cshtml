        <script>
            let videoUploaded = false;  // İlk başta video yüklenmiş değil.

            function triggerVideoInput() {
                if (videoUploaded) {
                    alert("Sadece bir video yüklenebilir.");
                    return;
                }
                document.getElementById('UploadedVideos').click();
            }

            document.getElementById('videoUploadArea').addEventListener('mouseover', function () {
                const tooltip = this.querySelector('.tooltip-text');
                tooltip.classList.remove('d-none');
            });

            document.getElementById('videoUploadArea').addEventListener('mouseout', function () {
                const tooltip = this.querySelector('.tooltip-text');
                tooltip.classList.add('d-none');
            });

            function previewVideos(input) {
                const video = input.files[0];

                // Video boyutu kontrolü (200MB)
                if (video.size > 200 * 1024 * 1024) {
                    document.getElementById('sizeWarning').style.display = 'block';
                    input.value = ''; // Yüklenmişse input'u sıfırlayın.
                    return;
                } else {
                    document.getElementById('sizeWarning').style.display = 'none';
                }

                // Video süresi kontrolü (2 dakika)
                const videoElement = document.createElement('video');
                videoElement.preload = 'metadata';
                videoElement.onloadedmetadata = function () {
                    const duration = videoElement.duration;
                    if (duration > 120) {  // 120 saniye = 2 dakika
                        document.getElementById('durationWarning').style.display = 'block';
                        input.value = ''; // Yüklenmişse input'u sıfırlayın.
                        return;
                    } else {
                        document.getElementById('durationWarning').style.display = 'none';
                    }

                    if (videoUploaded) {
                        alert("Sadece bir video yüklenebilir.");
                        input.value = ''; // Yüklenmişse input'u sıfırlayın.
                        return;
                    }

                    const videoPreview = document.getElementById('videoPreview');
                    videoPreview.innerHTML = '';  // Eski video önizlemesi temizleniyor

                    const files = Array.from(input.files);

                    files.forEach((file) => {
                        const videoUploadArea = document.getElementById('videoUploadArea');
                        videoUploadArea.innerHTML = '<div class="uploading-text">0%</div>'; // Yüzde gösterimi başladı
                        let progress = 0;
                        videoUploaded = true;

                        // Yükleme süresi rastgele belirleniyor (35 ile 60 saniye arasında)
                        const totalTime = Math.floor(Math.random() * (60 - 35 + 1)) + 35; // 35-60 saniye arasında
                        let intervalTime = (totalTime * 1000) / 100; // Başlangıçta %1 artış için 100 dilim

                        // Sağ alt köşede ilerleme göstergeyi başlat
                        const uploadProgressDisplay = document.getElementById('uploadProgressDisplay');
                        uploadProgressDisplay.style.display = 'block';
                        uploadProgressDisplay.innerText = `Video Yükleniyor: 0%`;

                        const interval = setInterval(() => {
                            // Rastgele hız değişimlerine karar verelim
                            let speedFactor = 1; // Normal hız başlangıç
                            let randomEffect = Math.random();

                            if (randomEffect < 0.3) { // %30 ihtimalle duraklama efekti
                                speedFactor = 0; // Yükleme duruyor
                                setTimeout(() => {
                                    progress++; // Duraklama bitince ilerlemeyi başlat
                                }, Math.floor(Math.random() * (2000 - 1000 + 1)) + 1000); // 1-2 saniye arası duraklama
                            } else if (randomEffect < 0.6) { // %30 ihtimalle hızlanma efekti
                                speedFactor = 1.5; // Hızlandır
                            } else if (randomEffect < 0.9) { // %30 ihtimalle yavaşlama efekti
                                speedFactor = 0.5; // Yavaşlat
                            }

                            // Yükleme işlemi zamanla devam eder
                            if (progress <= 100) {
                                progress += speedFactor; // İlerlemenin hızını yavaşlatma veya hızlandırma
                            }

                            // Yükleme tamamlandığında işlemi durdur
                            if (progress >= 100) {
                                progress = 100;
                                clearInterval(interval);

                                const reader = new FileReader();
                                reader.onload = function (e) {
                                    const div = document.createElement('div');
                                    div.classList.add('preview-container');
                                    div.innerHTML = `
                                            <video controls>
                                                <source src="${e.target.result}" type="video/mp4">
                                                Your browser does not support the video tag.
                                            </video>
                                            <span class="delete-icon" onclick="removeVideo(this)">✖</span>
                                        `;
                                    videoPreview.appendChild(div);
                                };
                                reader.readAsDataURL(file);

                                videoUploadArea.innerHTML = '<i class="fas fa-video video-icon"></i>'; // Orijinal ikon geri yüklendi

                                // %100 tamamlandığında 3 saniye sonra sağ alt göstergeyi gizle
                                setTimeout(() => {
                                    uploadProgressDisplay.style.display = 'none';
                                }, 3000); // 3000 ms = 3 saniye
                            }

                            // Tam sayı olarak yuvarlama
                            const roundedProgress = Math.floor(progress); // Yüzdeyi tam sayıya yuvarla

                            const uploadingText = videoUploadArea.querySelector('.uploading-text');
                            if (uploadingText) {
                                uploadingText.innerText = `${roundedProgress}%`; // Yuvarlanmış değeri göster
                            }
                            uploadProgressDisplay.innerText = `Video Yükleniyor: ${roundedProgress}%`;

                        }, intervalTime);
                    });

                };
                videoElement.src = URL.createObjectURL(video);
            }

