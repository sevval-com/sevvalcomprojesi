    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewData["Title"] - Sevval.com</title>

    <script>
        // Global API base: localhost uses direct API; production uses relative /api (behind reverse proxy)
        window.API_BASE = (location.hostname === 'localhost' || location.hostname === '127.0.0.1')
            ? 'https://localhost:44347/api/v1'
            : '/api/proxy';
    </script>

